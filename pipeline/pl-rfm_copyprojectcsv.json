									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "ProjectFlag",
									"type": "Boolean",
									"physicalType": "bit"
								}
							},
							{
								"sink": {
									"name": "YearSubmitted",
									"type": "Int32",
									"physicalType": "int"
								}
							},
							{
								"sink": {
									"name": "ProjectName",
									"type": "String",
									"physicalType": "varchar"
								}
							},
							{
