{
	"name": "pl-rfm_copyprojectcsv",
	"properties": {
		"description": "Copies OS project data from csv file to Azure SQL table",
		"activities": [
			{
				"name": "Copy OS Projects",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
