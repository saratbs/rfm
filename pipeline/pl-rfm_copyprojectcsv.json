									"name": "ProjectAreaRegion",
									"type": "String",
									"physicalType": "varchar"
								}
							},
							{
								"source": {
									"name": "Scenario",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "Scenario",
									"type": "String",
									"physicalType": "varchar"
								}
							},
							{
								"source": {
									"name": "Phase",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "Phase",
									"type": "String",
									"physicalType": "varchar"
								}
